{
    "name": "kaio/zippa",
    "description": "Projeto Zippa",
    "type": "project",
    "require": {
        "doctrine/orm": "^3.5",
        "doctrine/dbal": "^4.3",
        "symfony/cache": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kaio Daniel Medeiros",
            "email": "kaiodanielmedeirosramos@gmail.com"
        }
    ],
    "scripts": {
        "create-db": "php bin/doctrine orm:schema-tool:create",
        "update-db": "php bin/doctrine orm:schema-tool:update --force",
        "start": "php -S localhost:8000 -t public"
    }
}